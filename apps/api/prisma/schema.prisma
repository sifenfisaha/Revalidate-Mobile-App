  id        BigInt    @id @default(autoincrement())
  email     String    @db.VarChar(255)
  otp       String    @db.VarChar(6)
  expires_at DateTime
  used      Boolean   @default(false)
  created_at DateTime  @default(now())

  @@index([email])
  @@index([expires_at])
  @@map("email_otps")
}
