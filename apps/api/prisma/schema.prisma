  id             BigInt               @id @default(autoincrement()) @db.UnsignedBigInt
  user_id        BigInt               @db.UnsignedBigInt
  hospital       BigInt               @db.UnsignedBigInt
  date           DateTime             @db.Date
  mode           String?              @db.VarChar(200)
  number_hours   String               @db.VarChar(200)
  work_settings  String               @db.VarChar(200)
  scope_practice String               @db.VarChar(200)
  registration   String               @db.VarChar(200)
  hourly_rate    Decimal              @default(0.00) @db.Decimal(18, 2)
  description    String?              @db.Text
  document       String?              @db.VarChar(200)
  status         working_hours_status @default(dbgenerated("0"))
  reset          working_hours_reset  @default(dbgenerated("0"))
  created_at     DateTime             @default(now()) @db.Timestamp(0)
  updated_at     DateTime             @default(now()) @db.Timestamp(0)
  earnings       earnings[]
  users          users                @relation(fields: [user_id], references: [id], onDelete: Cascade, map: "working_hours_user_id_foreign")

  @@index([user_id], map: "working_hours_user_id_foreign")
}

