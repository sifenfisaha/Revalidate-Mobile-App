model email_otps {
  id         BigInt   @id @default(autoincrement())
  email      String   @db.VarChar(255)
  otp        String   @db.VarChar(10)
  used       Boolean  @default(false)
  expires_at DateTime @db.DateTime
  created_at DateTime @default(now()) @db.Timestamp(0)

  @@index([email])
  @@map("email_otps")
}
